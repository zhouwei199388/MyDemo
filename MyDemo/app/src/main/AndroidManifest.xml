<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="zw.com.mydemo">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".animation.FrameAnimationActivity" />
        <activity android:name=".animation.TweendAnimationActivity" />
        <activity android:name=".animation.PropertyAnimationActivity" />
        <activity android:name=".threadPool.ThreadPoolActivity" />
        <activity android:name=".socket.SocketActivity" />
        <activity android:name=".liveData.LiveDataActivity" />

    </application>
</manifest>
